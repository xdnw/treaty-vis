{
  "name": "discmcp-timelapse-web",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "data:archive:scrape": ".venv\\Scripts\\python.exe .\\scripts\\archive_scrape.py",
    "data:archive:to-treaties": ".venv\\Scripts\\python.exe .\\scripts\\archive_to_treaties.py",
    "data:bot:scrape": ".venv\\Scripts\\python.exe .\\scripts\\bot_scrape.py",
    "data:bot:to-treaties": ".venv\\Scripts\\python.exe .\\scripts\\bot_to_treaties.py",
    "data:sync": "node ./scripts/sync-data.mjs",
    "data:generate": ".venv\\Scripts\\python.exe .\\scripts\\generate_timelapse_data.py --infer-expiry-cancels --infer-deletion-cancels --collapse-churn",
    "data:generate:top50": ".venv\\Scripts\\python.exe .\\scripts\\generate_timelapse_data.py --infer-expiry-cancels --infer-deletion-cancels --top50-mode strict --output .\\public\\data\\treaty_changes_reconciled_top50_strict.msgpack --summary-output .\\public\\data\\treaty_changes_reconciled_top50_strict_summary.msgpack --flags-output .\\public\\data\\treaty_changes_reconciled_top50_strict_flags.msgpack --collapse-churn",
    "data:generate:dry-run": ".venv\\Scripts\\python.exe .\\scripts\\generate_timelapse_data.py --infer-expiry-cancels --infer-deletion-cancels --collapse-churn --dry-run",
    "data:score": ".venv\\Scripts\\python.exe .\\scripts\\score.py --download-missing-alliance-archives --pretty",
    "data:refresh": "npm run data:generate && npm run data:score && npm run data:flags && npm run data:sync",
    "data:flags": ".venv\\Scripts\\python.exe .\\scripts\\flags.py --download-missing-alliance-archives --pretty",
    "data:flags:legacy": ".venv\\Scripts\\python.exe .\\scripts\\flags.py --legacy-backfill-only --legacy-imgbb-only --pretty",
    "test:py": ".venv\\Scripts\\python.exe -m unittest scripts/test_generate_timelapse_data.py"
  },
  "dependencies": {
    "@msgpack/msgpack": "^3.1.2",
    "date-fns": "^4.1.0",
    "echarts": "^5.6.0",
    "echarts-for-react": "^3.0.2",
    "graphology": "^0.26.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.2.0",
    "sigma": "^3.0.2",
    "zod": "^3.24.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@types/node": "^22.13.5",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.20",
    "gh-pages": "^6.3.0",
    "jsdom": "^28.1.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vitest": "^3.0.7"
  }
}
